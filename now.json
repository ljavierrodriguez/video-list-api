{
  "version": 2,
  "name": "video list api",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/playlists/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/playlists/(.*)/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/playlists/(.*)/video/(.*)/delete",
      "dest": "/api/index.js"
    }
  ]
}
